{
  "name": "triptalk",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:fetch": "node ./tests/fetchTest.js",
    "test:production": "node ./tests/fetchTestProduction.js",
    "lint": "vue-cli-service lint",
    "ios": "rm -rf ios && ionic build %% ionic capacitor add ios &&  ionic capacitor open ios",
    "icons:ios": "cordova-res ios --icon-source './resources/icon_ios.png' --skip-config --copy ",
    "icons:android": "cordova-res android --icon-source './resources/icon_android.png' --skip-config --copy ",
    "open:ios": "rm -f ./ios/App/._App.xcworkspace && ionic capacitor open ios",
    "open:android": "ionic capacitor open android",
    "sync:full": "ionic build && npx cap sync",
    "sync:android": "npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "serve:external": "ionic cap run ios -l --external --livereload-url",
    "media:ios": "npm install cordova-plugin-media@2.1.0",
    "media:android": "npm install cordova-plugin-media@latest"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.6",
    "@capacitor/core": "2.4.3",
    "@capacitor/ios": "^2.4.6",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/geolocation": "^5.30.0",
    "@ionic-native/media": "^5.30.0",
    "@ionic-native/screen-orientation": "^5.31.1",
    "@ionic-native/text-to-speech": "^5.30.0",
    "@ionic/vue": "^5.5.2",
    "@ionic/vue-router": "^5.5.2",
    "axios": "^0.21.1",
    "clamp-js-main": "^0.11.6",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-file": "^4.3.3",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-media": "^2.1.0",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "cordova-plugin-tts": "^0.2.3",
    "cordova-res": "^0.14.0",
    "core-js": "^3.8.2",
    "es6-promise-plugin": "^4.2.2",
    "gsap": "^3.6.0",
    "lottie-web": "^5.7.6",
    "pageviews": "^1.5.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "wtf_wikipedia": "^8.5.1"
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.3",
    "@ionic/lab": "3.2.10",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-e2e-cypress": "^4.5.10",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-plugin-router": "^4.5.10",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-beta.14",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.4.1",
    "vue-jest": "^5.0.0-alpha.8"
  },
  "description": "An Ionic project"
}